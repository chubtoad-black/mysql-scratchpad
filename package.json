{
    "name": "mysql-scratchpad",
    "displayName": "mysql-scratchpad",
    "description": "Easy mysql statement running and output.",
    "version": "0.0.1",
    "publisher": "jblack",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:mysql-scratchpad.mysqlConnect"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
        {
            "command": "mysql-scratchpad.executeCurrentLine",
            "title": "Execute statement under cursor",
            "category": "MySQL"
        },{
            "command": "mysql-scratchpad.mysqlConnect",
            "title": "Connect",
            "category": "MySQL"
        },{
            "command": "mysql-scratchpad.openScratchpad",
            "title": "Open Scratchpad",
            "category": "MySQL"
        },{
            "command": "mysql-scratchpad.mysqlDisconnect",
            "title": "Disconnect",
            "category": "MySQL"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "mysql": "2.13.0"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}